			<flow-ref doc:name="mainFlow" doc:id="4ab10ee0-ca59-4bb9-9ef7-f49766882935" name="mainFlow"/>
		</munit:execution>
		<munit:validation >
			<munit-tools:assert-equals doc:name="Assert equals" doc:id="661cfc23-0fa9-46a0-ae1a-c7f32b1eb7de" actual="#[payload]" expected='#["hello world"]'/>
		</munit:validation>
	</munit:test>
